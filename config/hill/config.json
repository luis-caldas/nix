{
  "boot": {
    "timeout": 1
  },
  "system": {
    "hostname": "hill",
    "layout": "gb",
    "numlock": true
  },
  "services": {
    "ssh": true,
    "docker": true,
    "printing": true,
    "startup": {
      "permit": [
        "/sys/devices/system/cpu/cpu*/cpufreq/*governor"
      ]
    }
  },
  "graphical": {
    "enable": true,
    "drivers": [ "amdgpu" ],
    "trackpoint": { "enable": true },
    "touchpad": { "enable": true },
    "production": {
      "audio": true,
      "models": true,
      "software": true,
      "business": true,
      "electronics": true
    }
  },
  "games": true,
  "audio": true,
  "bluetooth": true
}
