{
  "boot": {
    "timeout": 1
  },
  "system": {
    "hostname": "hill"
  },
  "net": {
    "id": "1ff99666"
  },
  "services": {
    "ssh": true,
    "docker": true,
    "printing": true,
    "fingerprint": true,
    "startup": {
      "permit": [
        "/sys/devices/system/cpu/cpu*/cpufreq/*governor"
      ]
    }
  },
  "graphical": {
    "enable": true,
    "drivers": [ "intel" ],
    "trackpoint": { "enable": true },
    "touchpad": { "enable": true },
    "production": true,
    "prod3d": true,
    "touch": true,
    "displays": [{
      "scale": 1, "display": ":0", "extraCommands": [
        "swap-back-and-slash",
	"xinput map-to-output 9 eDP1"
      ]
    }]
  },
  "games": true,
  "audio": true,
  "bluetooth": true
}
