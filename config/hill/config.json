{
  "boot": {
    "timeout": 1
  },
  "system": {
    "hostname": "hill"
  },
  "services": {
    "ssh": true,
    "docker": true,
    "printing": true,
    "fingerprint": true,
    "startup": {
      "permit": [
        "/sys/devices/system/cpu/cpu*/cpufreq/*governor"
      ]
    }
  },
  "graphical": {
    "enable": true,
    "drivers": [ "intel" ],
    "trackpoint": { "enable": true },
    "touchpad": { "enable": true },
    "production": {
      "audio": true,
      "video": true,
      "models": true,
      "electronics": true
    },
    "touch": true,
    "display": { "extraCommands": [ "reneoslashtoback" ] }
  },
  "games": true,
  "audio": true,
  "bluetooth": true
}
