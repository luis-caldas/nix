{
  "boot": {
    "override": true,
    "timeout": 1
  },
  "kernel": {
    "params": [ "console=tty1" ]
  },
  "zram": true,
  "user": { "name": "lakituen" },
  "system": {
    "hostname": "lap"
  },
  "services": {
    "ssh": true,
    "printing": true,
    "startup": {
      "permit": [
        "/sys/class/devfreq/*/*governor",
        "/sys/devices/system/cpu/cpu*/cpufreq/*governor"
      ]
    }
  },
  "graphical": {
    "enable": true,
    "drivers": ["panfrost"],
    "compositor": false,
    "touchpad": { "enable": true },
    "display": { "extraCommands": ["cneogovernor restore"] }
  },
  "audio": true
}
