{
  "boot": {
    "override": true,
    "timeout": 1
  },
  "x86_64": false,
  "kernel": {
    "params": ["console=ttyS2,1500000n8", "console=tty0"]
  },
  "zram": true,
  "system": {
    "hostname": "lap",
    "permit": [
      "/sys/class/devfreq/*/{,scaling_}governor",
      "/sys/devices/system/cpu/cpu*/cpufreq/{,scaling_}governor"
    ]
  },
  "net": {
    "id": "29387412"
  },
  "services": {
    "ssh": true
  },
  "games": true,
  "graphical": {
    "enable": true,
    "drivers": ["panfrost"],
    "touchpad": true,
    "displays": [
      { "scale": 1, "display": ":0", "extraCommands": ["swap-back-and-slash", "neogov restore"] }
    ]
  },
  "audio": true
}
