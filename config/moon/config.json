{
  "boot": {
    "timeout": 1
  },
  "system": {
    "hostname": "moon"
  },
  "services": {
    "ssh": true,
    "printing": true,
    "startup": {
      "permit": [
        "/sys/devices/system/cpu/cpu*/cpufreq/*governor",
        "/dev/nvme1n1"
      ],
      "create": [
        "/dev/shm/looking-glass"
      ]
    }
  },
  "graphical": {
    "enable": true,
    "drivers": [ "amdgpu" ],
    "production": {
      "audio": true,
      "video": true,
      "models": true,
      "electronics": true
    }
  },
  "games": true,
  "audio": true,
  "bluetooth": true
}
