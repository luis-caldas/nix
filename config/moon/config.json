{
  "boot": {
    "timeout": 1
  },
  "system": {
    "hostname": "moon",
    "numlock": true
  },
  "net": {
    "id": "19709431"
  },
  "services": {
    "ssh": true,
    "printing": true,
    "startup": {
      "permit": [
        "/sys/devices/system/cpu/cpu*/cpufreq/*governor",
        "/dev/nvme1n1"
      ],
      "create": [
        "/dev/shm/looking-glass"
      ]
    }
  },
  "graphical": {
    "enable": true,
    "drivers": ["amdgpu"],
    "display": {
      "scale": 2,
      "extraCommands": [
        "xrandr --dpi 192x192",
        "xrandr --output DisplayPort-0 --mode 1920x1080 --rate 144 --scale 2x2 --pos 0x0 --primary",
        "xrandr --output DisplayPort-1 --mode 3840x2160 --rate 60 --scale 1x1 --pos 3840x0"
      ]
    },
    "production": {
      "audio": true,
      "models": true,
      "electronics": true
    }
  },
  "games": true,
  "audio": true,
  "bluetooth": true
}
